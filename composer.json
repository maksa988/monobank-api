{
    "name": "maksa988/monobank-api",
    "description": "PHP client for Mobobank API",
    "license": "MIT",
    "authors": [
        {
            "name": "maksa988",
            "email": "maksa988ua@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Maksa988\\MonobankAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maksa988\\MonobankAPI\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
